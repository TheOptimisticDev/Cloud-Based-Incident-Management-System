{
  "extends": "eslint-config-airbnb-es5",
  "rules": {
    "linebreak-style": 0,
    "indent": [
      1,
      "tab",
      {
        "SwitchCase": 1
      }
    ],
    "no-tabs": 0,
    "comma-dangle": [
      "error",
      {
        "functions": "never"
      }
    ],
    "semi": 1,
    "prefer-template": 0,
    "no-use-before-define": [
      1,
      {
        "functions": true,
        "classes": true
      }
    ],
    "prefer-arrow-callback": 0,
    "no-undef": 1,
    "no-mixed-spaces-and-tabs": [
      "error",
      "smart-tabs"
    ],
    "keyword-spacing": [
      "error",
      {
        "overrides": {
          "return": {
            "after": true
          },
          "if": {
            "after": false
          },
          "else": {
            "after": false,
            "before": false
          },
          "try": {
            "after": false,
            "before": false
          },
          "catch": {
            "after": false,
            "before": false
          },
          "switch": {
            "after": false,
            "before": false
          },
          "for": {
            "after": false,
            "before": false
          },
          "while": {
            "after": false,
            "before": false
          },
          "do": {
            "after": false,
            "before": false
          }
        }
      }
    ],
    "no-var": 0,
    "one-var": [
      "error",
      "never"
    ],
    "space-before-blocks": [
      "error",
      "never"
    ],
    "space-before-function-paren": [
      "error",
      "never"
    ],
    "no-param-reassign": 1,
    "object-shorthand": 0,
    "vars-on-top": 0,
    "global-require": 1,
    "import/no-extraneous-dependencies": 1,
    "valid-jsdoc": 1,
    "spaced-comment": 1,
    "capitalized-comments": 1,
    "no-empty": [
      "error",
      {
        "allowEmptyCatch": true
      }
    ],
    "no-unused-vars": 1,
    "consistent-return": 1,
    "radix": 1,
    "no-shadow": [
      "error",
      {
        "allow": [
          "config",
          "done",
          "cb",
          "err"
        ]
      }
    ]
  }
}
